{
	"name": "ROBLOX Studio",
	"description": "ROBLOX Studio in the browser",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"tabs",
		"cookies",
		"scripting"
	],
	"host_permissions": [
		"https://*.roblox.com/*"
	],
	"action": {},
	"background": {
		"service_worker": "scripts/backend.js"
	},
	"icons": {
		"16": "unicorn.png",
		"32": "unicorn.png",
		"48": "unicorn.png",
		"128": "unicorn.png"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
	}
}